    "@types/request": "~2.48.1",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-consistent-codestyle": "^1.16.0",
    "typescript": "~3.8.0"
  },
  "lint-staged": {
    "src/**/*.ts": [
